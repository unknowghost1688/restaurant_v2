@model IEnumerable<RemarkTypeInfo>

@if (Model.Count() > 0)
{
    <ul>
        @foreach (var item in Model)
        {
           <li><a href="" onclick="loadRemark('@item.RemarkTypeCode')">@item.RemarkDescription</a></li> 
        }
    </ul>
}
else
{
    <ul>
        <li>没有设置备注类型</li>
    </ul>
}   
