@model IEnumerable<RemarkInfo>

@if (Model.Count()>0)
{
    <ul  class="require">
        @foreach (var item in Model)
        {
            <li id="@item.SerialNumber" class="extrarqr"><a href="" onclick="selectRemark('@item.SerialNumber')">@item.Remark</a></li>
        }
    </ul>
}else
{
<ul>
    <li>此类型没有设置备注</li>
</ul>
}